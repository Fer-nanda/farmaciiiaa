<Window x:Class="Farmacia.GUI.Administrador.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Farmacia.GUI.Administrador"
        mc:Ignorable="d"
        Title="Mi querido Enfermito" Height="500" Width="300" FontFamily="Kristen ITC">
    <Grid>
		<Image Source="Solid-Surface-Porcelanosa-Projects-Farmacia-Gran-Via_02.png" Stretch="Fill"/>
		<Button  Height="50" Width="100" x:Name="btnAgregar" Click="btnAgregar_Click" Margin="101,144,91,275" Content="Agregar" Background="#FF88B9EA" FontWeight="Bold" FontFamily="Comic Sans MS" FontSize="18"/>
		<Button  Height="50" Width="100" x:Name="btnVenta" Click="btnVenta_Click" Margin="101,247,91,172" Content="Venta" Background="#FF88B9EA" FontWeight="Bold" FontFamily="Comic Sans MS" FontSize="16"/>
		<Button  Height="50" Width="100" x:Name="btnSalir" Click="btnSalir_Click" Margin="101,353,91,66" Content="Salir" Background="#FF88B9EA" FontFamily="Comic Sans MS" FontSize="16" FontWeight="Bold"/>
	</Grid>
</Window>
